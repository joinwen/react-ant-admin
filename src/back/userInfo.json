{
  "menus": [
    {
      "path": "/home",
      "name": "主页",
      "icon": "HomeFilled"
    },
    {
      "path": "/form",
      "name": "表单",
      "icon": "DatabaseFilled",
      "children": [
        {
          "path": "/form/basic",
          "name": "基础表单"
        },
        {
          "path": "/form/step",
          "name": "分布表单"
        },
        {
          "path": "/form/advanced",
          "name": "高级表单"
        }
      ]
    },
    {
      "name": "表格",
      "path": "/table",
      "icon": "UnorderedListOutlined",
      "children": [
        {
          "name": "基础表格",
          "path": "/table/basic"
        },
        {
          "name": "高级表格",
          "path": "/table/advanced"
        }
      ]
    },
    {
      "path": "/result",
      "name": "结果页",
      "icon": "TagFilled",
      "children": [
        {
          "path": "/result/success",
          "name": "成功页"
        },
        {
          "path": "/result/failure",
          "name": "失败页"
        }
      ]
    },
    {
      "path": "/exception",
      "name": "异常",
      "icon": "BugFilled",
      "children": [
        {
          "path": "/exception/500",
          "name": "Error"
        },
        {
          "path": "/exception/404",
          "name": "Not Found"
        },
        {
          "path": "/exception/403",
          "name": "Forbidden"
        }
      ]
    },
    {
      "path": "/chart",
      "name": "图表",
      "icon": "PieChartFilled",
      "children": [
        {
          "path": "/chart/echarts",
          "name": "Echarts"
        },
        {
          "path": "/chart/d3",
          "name": "D3"
        }
      ]
    }
  ],
  "permissions": [
    "home",
    "table",
    "table:basic",
    "table:step",
    "table:advanced",
    "result",
    "result:success",
    "result:failure",
    "exception",
    "exception:error",
    "exception:not-found",
    "exception:forbidden"
  ],
  "routes": [
    {
      "path": "/",
      "component": "BasicLayout",
      "title": "主页",
      "children": [
        {
          "path": "/home",
          "component": "home/Home",
          "title": "首页"
        },
        {
          "path": "/form",
          "component": "BlankLayout",
          "title": "表单",
          "children": [
            {
              "path": "/form/basic",
              "component": "form/basic-form/BasicForm",
              "title": "基础表单"
            },
            {
              "path": "/form/step",
              "component": "form/step-form/StepForm",
              "title": "分步表单"
            },
            {
              "path": "/form/advanced",
              "component": "form/advanced-form/AdvancedForm",
              "title": "高级表单"
            }
          ]
        },
        {
          "path": "/table",
          "component": "BlankLayout",
          "title": "表格",
          "children": [
            {
              "path": "/table/basic",
              "component": "table/condition-table/ConditionTable",
              "title": "基础表格"
            },
            {
              "path": "/table/advanced",
              "component": "table/advanced-table/AdvancedTable",
              "title": "高级表格"
            }
          ]
        },
        {
          "path": "/result",
          "component": "BlankLayout",
          "title": "结果页",
          "children": [
            {
              "path": "/result/success",
              "component": "result/success/Success",
              "title": "成功页"
            },
            {
              "path": "/result/failure",
              "component": "result/failure/Failure",
              "title": "失败页"
            }
          ]
        },
        {
          "path": "/exception",
          "component": "BlankLayout",
          "title": "异常页",
          "children": [
            {
              "path": "/exception/500",
              "component": "exception/error/Error",
              "title": "内部错误"
            },
            {
              "path": "/exception/404",
              "component": "exception/not-found/NotFound",
              "title": "未找到"
            },
            {
              "path": "/exception/403",
              "component": "exception/forbidden/Forbidden",
              "title": "无权限"
            }
          ]
        },
        {
          "path": "/chart",
          "component": "BlankLayout",
          "title": "图表",
          "children": [
            {
              "path": "/chart/echarts",
              "component": "chart/echarts/Echarts",
              "title": "echarts"
            },
            {
              "path": "/chart/d3",
              "component": "chart/d3/D3",
              "title": "D3"
            }
          ]
        }
      ]
    }
  ]
}
